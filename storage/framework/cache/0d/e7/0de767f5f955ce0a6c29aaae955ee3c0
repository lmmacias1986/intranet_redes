    function onObservacion(){
        return Redirect::to('observacion_perfil/'.$this->param('id'));
    }
    function onStart(){
		$perfil = DB::table('perfil')->where('id','=', $this->param('id'))->first();
    	$this['perfil'] = $perfil;

        $observaciones_perf = DB::table('observaciones_perfil')->where('id_perfil','=', $this->param('id'))->orderBy('fecha', 'desc')->paginate(10);
        $this['observaciones_perf'] = $observaciones_perf;        

		$empresa = Db::table('portada_perfiles')
		    ->join('guia_cliente', 'portada_perfiles.id_guia', '=', 'guia_cliente.id')
		    ->select('guia_cliente.empresa', 'guia_cliente.id')
		    ->where('portada_perfiles.id','=', $perfil->id_portada)
		    ->first();
	    $this['empresa'] = $empresa; 
		$this['id_portada'] = $perfil->id_portada;
	}

	function onDelete(){
	    Db::table('perfil')->where('id', '=', $this->param('id'))->delete();
	    $this['mensaje'] = "Registro eliminado correctamente";
	    return Redirect::to('ver_perfiles/'.input('id_portada'));
	}
    function onSave()
    {
        $validator = false;
        if (input('cargo') == ""){
            $this['error'] = "Debe ingresar el nombre del cargo.";
        }elseif(input('salario')==""){   
            $this['error'] = "Debe ingresar el salario.";
        }elseif(input('horario')==""){
            $this['error'] = "Debe ingresar el horario.";
        }else{
           $validator= true;
        }

        if ($validator) {
           Db::table('perfil')->where('id', $this->param('id'))->update([
                'fecha_actualizacion' => date('Y-m-d'),
                'cargo' => input('cargo'),
                'rango_edad' => input('rango_edad'),
                'sexo' => input('sexo'),
